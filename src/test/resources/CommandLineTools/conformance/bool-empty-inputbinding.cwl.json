            "default": {
                "class": "File",
                "location": "args.py"
            },
            "inputBinding": {
                "position": -1
            }
        }
    ],
    "outputs": [
        {
            "id": "args",
            "type": {
                "type": "array",
                "items": "string"
            }
        }
    ],
    "baseCommand": "python",
    "arguments": [],
    "requirements": [
        {
            "class": "InlineJavascriptRequirement"
        }
    ]
}