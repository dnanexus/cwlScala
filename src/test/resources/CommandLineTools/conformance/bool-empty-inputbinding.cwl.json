{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "hints": [
        {
            "class": "DockerRequirement",
            "dockerPull": "python:2-slim"
        }
    ],
    "inputs": [
        {
            "id": "flag",
            "type": "boolean",
            "inputBinding": {}
        },
        {
            "id": "args.py",
