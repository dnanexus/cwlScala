{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "inputs": [
        {
            "type": "string",
            "id": "in"
        }
    ],
    "outputs": [
        {
            "type": "File",
            "outputBinding": {
                "glob": "out"
            },
            "id": "out"
        }
    ],
    "requirements": [
        {
            "envDef": {
                "TEST_ENV": "$(inputs.in)"
            },
            "class": "EnvVarRequirement"
        },
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "baseCommand": [
        "/bin/sh",
        "-c",
        "echo $TEST_ENV"
    ],
    "stdout": "out"
}