{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "inputs": [
        {
            "type": "File",
            "id": "file1"
        }
    ],
    "outputs": [
        {
            "type": "File",
            "outputBinding": {
                "glob": "output"
            },
            "id": "output"
        }
    ],
    "baseCommand": [
        "cat"
    ],
    "stdin": "$(inputs.file1.path)",
    "stdout": "output",
    "requirements": [
        {
            "class": "InlineJavascriptRequirement"
        }
    ]
}