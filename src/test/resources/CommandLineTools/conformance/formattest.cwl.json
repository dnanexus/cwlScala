{
    "$namespaces": {
        "edam": "http://edamontology.org/"
    },
    "cwlVersion": "v1.2",
    "class": "CommandLineTool",
    "doc": "Reverse each line using the `rev` command",
    "inputs": [
        {
            "type": "File",
            "inputBinding": {},
            "format": "edam:format_2330",
            "id": "input"
        }
    ],
    "outputs": [
        {
            "type": "File",
            "outputBinding": {
                "glob": "output.txt"
            },
            "format": "edam:format_2330",
            "id": "output"
        }
    ],
    "baseCommand": "rev",
    "stdout": "output.txt",
    "requirements": [
        {
            "class": "InlineJavascriptRequirement"
        }
    ]
}