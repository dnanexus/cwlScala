{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "requirements": [
        {
            "loadListing": "deep_listing",
            "class": "LoadListingRequirement"
        },
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "inputs": [
        {
            "type": "Directory",
            "id": "d"
        }
    ],
    "outputs": [
        {
            "type": "boolean",
            "outputBinding": {
                "outputEval": "$(inputs.d.listing.length === 1 && inputs.d.listing[0].listing.length === 1)"
            },
            "id": "out"
        }
    ],
    "baseCommand": "true"
}