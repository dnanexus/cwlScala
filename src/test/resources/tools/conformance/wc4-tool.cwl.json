{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "requirements": [
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "inputs": [
        {
            "type": "File",
            "inputBinding": {},
            "id": "file1"
        }
    ],
    "outputs": [
        {
            "id": "output",
            "type": "int",
            "outputBinding": {
                "glob": "output.txt",
                "loadContents": true,
                "outputEval": "${\n  var s = self[0].contents.split(/\\r?\\n/);\n  return parseInt(s[s.length-2]);\n}\n"
            }
        }
    ],
    "stdout": "output.txt",
    "baseCommand": "wc"
}