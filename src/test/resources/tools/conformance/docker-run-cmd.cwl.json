{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "requirements": [
        {
            "dockerPull": "bash:4.4.12",
            "class": "DockerRequirement"
        },
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "inputs": [],
    "outputs": [
        {
            "type": "File",
            "outputBinding": {
                "glob": "cow"
            },
            "id": "cow"
        }
    ],
    "baseCommand": [
        "-c",
        "echo 'moo' > cow"
    ]
}