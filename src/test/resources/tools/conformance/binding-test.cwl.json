{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "hints": [
        {
            "class": "DockerRequirement",
            "dockerPull": "python:2-slim"
        }
    ],
    "inputs": [
        {
            "id": "reference",
            "type": "File",
            "inputBinding": {
                "position": 2
            }
        },
        {
            "id": "reads",
            "type": {
                "type": "array",
                "items": "File",
                "inputBinding": {
                    "prefix": "-YYY"
                }
            },
            "inputBinding": {
                "position": 3,
                "prefix": "-XXX"
            }
        },
        {
            "id": "#args.py",
            "type": "File",
            "default": {
                "class": "File",
                "location": "args.py"
            },
            "inputBinding": {
                "position": -1
            }
        }
    ],
    "outputs": [
        {
            "type": {
                "type": "array",
                "items": "string"
            },
            "id": "args"
        }
    ],
    "baseCommand": "python",
    "arguments": [
        "bwa",
        "mem"
    ],
    "requirements": [
        {
            "class": "InlineJavascriptRequirement"
        }
    ]
}