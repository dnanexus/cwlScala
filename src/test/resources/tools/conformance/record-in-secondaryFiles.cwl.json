{
    "class": "CommandLineTool",
    "cwlVersion": "v1.2",
    "inputs": [
        {
            "type": {
                "type": "record",
                "fields": [
                    {
                        "type": "File",
                        "secondaryFiles": ".s2",
                        "name": "f1"
                    },
                    {
                        "type": {
                            "type": "array",
                            "items": "File"
                        },
                        "secondaryFiles": ".s3",
                        "name": "f2"
                    }
                ]
            },
            "id": "record_input"
        }
    ],
    "outputs": [],
    "baseCommand": "test",
    "arguments": [
        "-f",
        "$(inputs.record_input.f1.path).s2",
        "-a",
        "-f",
        "$(inputs.record_input.f2[0].path).s3",
        "-a",
        "-f",
        "$(inputs.record_input.f2[1].path).s3"
    ],
    "requirements": [
        {
            "class": "InlineJavascriptRequirement"
        }
    ]
}