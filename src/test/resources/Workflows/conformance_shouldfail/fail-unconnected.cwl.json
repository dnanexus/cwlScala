{
    "class": "Workflow",
    "cwlVersion": "v1.2",
    "inputs": [
        {
            "type": "string",
            "default": "hello inp1",
            "id": "inp1"
        },
        {
            "type": "string",
            "default": "hello inp2",
            "id": "inp2"
        }
    ],
    "outputs": [
        {
            "type": "string",
            "outputSource": "step1/out",
            "id": "out"
        }
    ],
    "steps": [
        {
            "in": [
                {
                    "source": "inp1",
                    "id": "in"
                },
                {
                    "source": "inp2",
                    "id": "in2"
                }
            ],
            "out": [
                "out"
            ],
            "run": {
                "class": "CommandLineTool",
                "inputs": [
                    {
                        "type": "string",
                        "id": "in"
                    }
                ],
                "outputs": [
                    {
                        "type": "string",
                        "outputBinding": {
                            "glob": "out.txt",
                            "loadContents": true,
                            "outputEval": "$(self[0].contents)"
                        },
                        "id": "out"
                    }
                ],
                "stdout": "out.txt",
                "arguments": [
                    "echo",
                    "$(inputs.in)",
                    "$(inputs.in2)"
                ],
                "requirements": [
                    {
                        "class": "InlineJavascriptRequirement"
                    }
                ],
                "id": "fail-unconnected.cwl:step_step1:fail-unspecified-input.cwl"
            },
            "id": "step1"
        }
    ],
    "requirements": [
        {
            "class": "SubworkflowFeatureRequirement"
        },
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "id": "fail-unconnected.cwl"
}