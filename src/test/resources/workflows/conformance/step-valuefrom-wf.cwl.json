{
    "class": "Workflow",
    "cwlVersion": "v1.2",
    "requirements": [
        {
            "class": "StepInputExpressionRequirement"
        },
        {
            "class": "SubworkflowFeatureRequirement"
        },
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "inputs": [
        {
            "type": {
                "name": "in",
                "type": "record",
                "fields": [
                    {
                        "name": "file1",
                        "type": "File"
                    }
                ]
            },
            "id": "in"
        }
    ],
    "outputs": [
        {
            "type": "int",
            "outputSource": "step2/output",
            "id": "count_output"
        }
    ],
    "steps": [
        {
            "run": {
                "class": "CommandLineTool",
                "inputs": [
                    {
                        "type": "File",
                        "id": "file1"
                    }
                ],
                "outputs": [
                    {
                        "type": "File",
                        "outputBinding": {
                            "glob": "output"
                        },
                        "id": "output"
                    }
                ],
                "baseCommand": [
                    "wc",
                    "-l"
                ],
                "stdin": "$(inputs.file1.path)",
                "stdout": "output",
                "requirements": [
                    {
                        "class": "InlineJavascriptRequirement"
                    }
                ]
            },
            "in": [
                {
                    "source": "in",
                    "valueFrom": "$(self.file1)",
                    "id": "file1"
                }
            ],
            "out": [
                "output"
            ],
            "id": "step1"
        },
        {
            "run": {
                "class": "ExpressionTool",
                "requirements": [
                    {
                        "class": "InlineJavascriptRequirement"
                    }
                ],
                "inputs": [
                    {
                        "type": "File",
                        "loadContents": true,
                        "id": "file1"
                    }
                ],
                "outputs": [
                    {
                        "type": "int",
                        "id": "output"
                    }
                ],
                "expression": "$({'output': parseInt(inputs.file1.contents)})"
            },
            "in": [
                {
                    "source": "step1/output",
                    "id": "file1"
                }
            ],
            "out": [
                "output"
            ],
            "id": "step2"
        }
    ]
}