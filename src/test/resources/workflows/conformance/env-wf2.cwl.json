{
    "class": "Workflow",
    "cwlVersion": "v1.2",
    "inputs": [
        {
            "type": "string",
            "id": "in"
        }
    ],
    "outputs": [
        {
            "type": "File",
            "outputSource": "step1/out",
            "id": "out"
        }
    ],
    "requirements": [
        {
            "envDef": {
                "TEST_ENV": "override"
            },
            "class": "EnvVarRequirement"
        },
        {
            "class": "SubworkflowFeatureRequirement"
        },
        {
            "class": "InlineJavascriptRequirement"
        }
    ],
    "steps": [
        {
            "run": {
                "class": "CommandLineTool",
                "inputs": [
                    {
                        "type": "string",
                        "id": "in"
                    }
                ],
                "outputs": [
                    {
                        "type": "File",
                        "outputBinding": {
                            "glob": "out"
                        },
                        "id": "out"
                    }
                ],
                "hints": {
                    "EnvVarRequirement": {
                        "envDef": {
                            "TEST_ENV": "$(inputs.in)"
                        }
                    }
                },
                "baseCommand": [
                    "/bin/sh",
                    "-c",
                    "echo $TEST_ENV"
                ],
                "stdout": "out",
                "requirements": [
                    {
                        "class": "InlineJavascriptRequirement"
                    }
                ]
            },
            "in": [
                {
                    "source": "in",
                    "id": "in"
                }
            ],
            "out": [
                "out"
            ],
            "id": "step1"
        }
    ]
}